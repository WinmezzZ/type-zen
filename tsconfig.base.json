{
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["ESNext"],
    "noEmit": true,
    "pretty": true,
    "allowJs": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "strictPropertyInitialization": false,
    "skipLibCheck": true
  }
}
